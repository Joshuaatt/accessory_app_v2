<%= render 'layouts/errors', object: @accessory %>

<%= bootstrap_form_for [ @model, @accessory ], :url => model_accessories_path, :html => { :multipart => true } do |f| %>
  <%= f.text_field :name %>
  <%= f.text_area :description, :rows => "10" %>
  <%= f.number_field :price, :step => 0.01 %>
  <%= f.number_field :parts_cost, :step => 0.01 %>
  <%= f.number_field :labor_cost, :step => 0.01 %>
  <%= f.text_field :part_number %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>
